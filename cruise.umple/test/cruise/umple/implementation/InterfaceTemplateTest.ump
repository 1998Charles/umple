namespace example;

interface ISomething
{
  constant String aVariable = aValue;
  constant Integer MAX = 3;
  
  String getCode (String[] aParam, Integer anotherParam);
  String isValid();
}

interface IGarbage
{
}

class Something
{
  isA ISomething;
}

class Garbage
{
  isA ISomething, IGarbage;
}